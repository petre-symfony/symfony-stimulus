<div class="list-group">
  {% for product in products %}
    <a href=
       "{{ path('app_product', { id: product.id}) }}"
       class="list-group-item list-group-item-action"
    >
      {{ product.name }}
    </a>
  {%  else %}
    <div class="list-group-item">
      No results found!
    </div>
  {% endfor %}

</div>
